[Server]
<% if @name -%>
  Name = <%= @name %>
<% end -%>
<% if @passive == 'no' -%>
<% if @host -%>
  Host = <%= @host %>
<% end -%>
<% if @port -%>
  Port = <%= @port %>
<% end -%>
<% if @bind -%>
  Bind = <%= @bind %>
<% end -%>
<% end -%>
<% if @local_password -%>
  MyPassword = <%= @local_password %>
<% end -%>
<% if @remote_password -%>
  PeerPassword = <%= @remote_password %>
<% end -%>
<% if @group -%>
  Group = <%= @group %>
<% end -%>
<% if @passive -%>
  Passive = <%= @passive %>
<% end -%>
<% if @ssl -%>
  SSLConnect = <%= @ssl %>
<% end -%>
<% if @service_mask -%>
  ServiceMask = <%= @service_mask %>
<% end -%>

