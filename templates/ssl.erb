[SSL]
<% if defined?(@certfile) -%>
  CertFile = <%= @certfile %>
<% end -%>
<% if defined?(@cipherlist) -%>
  CipherList = <%= @ciphers.join(' ') %>
<% end -%>
<% if defined?(@dhfile) -%>
  DHFile = <%= @dhfile %>
<% end -%>
<% if defined?(@keyfile) -%>
  KeyFile = <%= @keyfile %>
<% end -%>
<% if defined?(@keyfilepassword) -%>
  KeyFilePassword = <%= @keyfilepassword %>
<% end -%>
<% if defined?(@ports) -%>
  Ports = <%= @ports.join(',') %>
<% end -%>
